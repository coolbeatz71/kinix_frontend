{
    "name": "kinix_frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 8070",
        "build": "next build",
        "start": "next start",
        "type:check": "tsc",
        "lint": "eslint **/*.tsx **/*.ts",
        "lint:fix": "eslint **/*.tsx **/*.ts --fix",
        "prettier": "prettier --check --config package.json **/*.tsx **/*.ts",
        "prettier:fix": "prettier --write **/*.tsx **/*.ts",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.8.1",
        "@types/color-hash": "^1.0.2",
        "@types/crypto-js": "^4.1.1",
        "@types/react-lottie": "^1.2.6",
        "@types/react-redux": "^7.1.24",
        "@types/react-star-rating-component": "^1.4.1",
        "@types/redux-logger": "^3.0.9",
        "antd": "^4.23.2",
        "axios": "^0.19.2",
        "babel-plugin-import": "^1.13.3",
        "babel-plugin-istanbul": "^6.0.0",
        "color-hash": "^2.0.1",
        "crypto-js": "^4.1.1",
        "dayjs": "^1.10.4",
        "get-video-id": "^3.6.5",
        "http-status": "^1.5.1",
        "i18next": "^21.8.3",
        "lodash": "^4.17.15",
        "next": "^12.0.3",
        "next-compose-plugins": "^2.2.1",
        "next-plugin-antd-less": "^1.4.2",
        "next-pwa": "5.0.3",
        "next-redux-wrapper": "^7.0.5",
        "nprogress": "^0.2.0",
        "nuka-carousel": "4.8.4",
        "numeral": "^2.0.6",
        "query-string": "^7.1.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-horizontal-scrolling-menu": "^2.7.1",
        "react-i18next": "^11.16.9",
        "react-icons": "^4.3.1",
        "react-infinite-scroll-component": "^6.1.0",
        "react-infinite-scroll-hook": "^4.0.3",
        "react-jwt": "^1.1.6",
        "react-lottie": "^1.2.3",
        "react-player": "^2.11.0",
        "react-redux": "^8.0.1",
        "react-star-rating-component": "^1.4.1",
        "redux-logger": "^3.0.6",
        "redux-persist": "^6.0.0",
        "sass": "^1.26.5",
        "slick-carousel": "^1.8.1",
        "style-it": "^2.1.4",
        "use-dark-mode": "^2.3.1",
        "validator": "^13.7.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^12.3.1",
        "@next/eslint-plugin-next": "^11.1.0",
        "@types/lodash": "^4.14.154",
        "@types/nprogress": "^0.2.0",
        "@types/numeral": "0.0.28",
        "@types/react": "17.0.19",
        "@types/validator": "^13.7.2",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "cross-env": "^7.0.3",
        "enforce-branch-name": "^1.0.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-prettier-react": "^0.0.24",
        "eslint-config-react-app": "^6.0.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-config-standard-react": "^11.0.1",
        "eslint-import-resolver-node": "^0.3.4",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-flowtype": "^5.7.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.5",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "4.3.0",
        "lint-staged": "^10.5.4",
        "prettier": "^2.3.2",
        "typescript": "^4.4.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run type:check && lint-staged",
            "pre-push": "enforce-branch-name '^(ft|ch|bg)-[a-z]+(-[a-z]+)*$'"
        }
    },
    "lint-staged": {
        "*.+(jsx|js|json|css|scss|md|tsx|ts)": [
            "prettier --write"
        ],
        "*.+(js|jsx|tsx|ts)": [
            "eslint --fix"
        ]
    }
}
